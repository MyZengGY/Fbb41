        <activity android:name=".activity.MsgActivity1"
            android:theme="@style/AppTheme.jh"></activity>
        <activity android:name=".activity.GrdataActivity2"
            android:theme="@style/AppTheme.jh"></activity>
        <activity android:name=".activity.VipActivity2"
        android:theme="@style/AppTheme.jh"></activity>
        <activity android:name=".activity.UserActivity2"
            android:theme="@style/AppTheme.jh"></activity>
        <activity android:name=".activity.PassworkActivity2"
            android:theme="@style/AppTheme.jh"></activity>
        <activity android:name=".activity.AccountActivity1"
